{
  "name": "bbc.github.io",
  "version": "1.0.0",
  "description": "An index of all BBC public repositories on GitHub.",
  "main": "app.js",
  "scripts": {
    "build": "npm run update && webpack -p --progress && node bin/render.js",
    "watch": "webpack --watch --progress --colors",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "node bin/cli.js"
  },
  "keywords": [
    "bbc",
    "open source",
    "oss"
  ],
  "author": "",
  "config": {
    "orgs": {
      "whitelist": [
        "BBCOpenSource",
        "BBC-Childrens",
        "bbc-fm-nk-core-eng",
        "bbc-test",
        "fmtvp",
        "BBC-Location-Services",
        "bbc-contentdiscovery",
        "BBC-News-Labs",
        "BBC-Data",
        "myBBC",
        "bbcsnippets",
        "BBC-Knowlearn",
        "bbc-sport",
        "iplayer",
        "BBCVisualJournalism",
        "bbcarchdev",
        "bbcrd",
        "BBC-News",
        "BBC"
      ]
    }
  },
  "license": "Apache-2",
  "dependencies": {
    "es6-promise": "^2.0.0",
    "node-jsx": "^0.12.0",
    "octonode": "^0.6.10",
    "react": "^0.12.0"
  },
  "devDependencies": {
    "css-loader": "^0.9.0",
    "json-loader": "^0.5.1",
    "jsx-loader": "^0.12.1",
    "less": "^1.7.5",
    "less-loader": "^0.7.7",
    "style-loader": "^0.8.2",
    "webpack": "^1.4.13"
  }
}
